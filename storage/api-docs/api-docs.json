{
    "openapi": "3.0.0",
    "info": {
        "title": "API Semillero",
        "description": "API para el sistema de Semillero",
        "contact": {
            "email": "tu_email@ejemplo.com"
        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:8000",
            "description": "Servidor Local"
        },
        {
            "url": "/api"
        }
    ],
    "paths": {
        "/api/inscriptions": {
            "get": {
                "tags": [
                    "Inscripciones"
                ],
                "summary": "Listar inscripciones",
                "operationId": "0250ffc7483e5904b8dc7ce76a002f3c",
                "parameters": [
                    {
                        "name": "semillero_id",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "name": "usuario_id",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Lista de inscripciones"
                    },
                    "404": {
                        "description": "No se encontraron inscripciones"
                    }
                }
            },
            "post": {
                "tags": [
                    "Inscripciones"
                ],
                "summary": "Inscribir estudiante en un semillero",
                "operationId": "f23d4010d13f4d9b01e9be052dd44a4d",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "semillero_id",
                                    "usuario_id"
                                ],
                                "properties": {
                                    "semillero_id": {
                                        "type": "integer",
                                        "example": 1
                                    },
                                    "usuario_id": {
                                        "type": "integer",
                                        "example": 5
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Inscripción exitosa"
                    },
                    "409": {
                        "description": "Ya existe inscripción"
                    },
                    "422": {
                        "description": "Datos inválidos"
                    }
                }
            }
        },
        "/api/seedbeds": {
            "get": {
                "tags": [
                    "CRUD Semilleros"
                ],
                "summary": "Listar semilleros",
                "operationId": "7b80408baacc1a453d1a5562ec32fb05",
                "responses": {
                    "200": {
                        "description": "Lista de semilleros"
                    }
                }
            },
            "post": {
                "tags": [
                    "CRUD Semilleros"
                ],
                "summary": "Crear un nuevo semillero",
                "operationId": "9033c31af8b5e6ff566208bfc287b5c0",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "nombre",
                                    "descripcion",
                                    "programa_id",
                                    "profesor_id"
                                ],
                                "properties": {
                                    "nombre": {
                                        "type": "string",
                                        "example": "Semillero TIC"
                                    },
                                    "descripcion": {
                                        "type": "string",
                                        "example": "Grupo de investigación en tecnología"
                                    },
                                    "programa_id": {
                                        "type": "integer",
                                        "example": 2
                                    },
                                    "profesor_id": {
                                        "type": "integer",
                                        "example": 5
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Semillero creado correctamente"
                    }
                }
            }
        },
        "/api/seedbeds/{id}": {
            "get": {
                "tags": [
                    "CRUD Semilleros"
                ],
                "summary": "Consultar semillero por ID",
                "operationId": "aca0ef5424e776a9f3760f913cf00633",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Datos del semillero"
                    },
                    "404": {
                        "description": "Semillero no encontrado"
                    }
                }
            },
            "put": {
                "tags": [
                    "CRUD Semilleros"
                ],
                "summary": "Actualizar un semillero existente",
                "operationId": "2f291b521480041d4368c0483a21cd99",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "nombre",
                                    "descripcion",
                                    "programa_id",
                                    "profesor_id"
                                ],
                                "properties": {
                                    "nombre": {
                                        "type": "string",
                                        "example": "Semillero Redes"
                                    },
                                    "descripcion": {
                                        "type": "string",
                                        "example": "Nuevo enfoque en redes y protocolos"
                                    },
                                    "programa_id": {
                                        "type": "integer",
                                        "example": 1
                                    },
                                    "profesor_id": {
                                        "type": "integer",
                                        "example": 8
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Semillero actualizado"
                    },
                    "404": {
                        "description": "Semillero no encontrado"
                    }
                }
            },
            "delete": {
                "tags": [
                    "CRUD Semilleros"
                ],
                "summary": "Eliminar un semillero",
                "operationId": "bf4639a036db7c2564339d383cf6d3e9",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Semillero eliminado correctamente"
                    },
                    "404": {
                        "description": "Semillero no encontrado"
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "bearerFormat": "JWT",
                "scheme": "bearer"
            }
        }
    },
    "tags": [
        {
            "name": "CRUD Semilleros",
            "description": "Operaciones relacionadas con el CRUD de semilleros"
        },
        {
            "name": "Inscripciones",
            "description": "Inscripciones"
        }
    ]
}